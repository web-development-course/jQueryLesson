<script>
  $(function() {
    var selector = ".page-container-sample .thumbnail";
    $(selector).click(function() {
      $(this).clone().insertAfter($(this)).find('.element').html('new');
    });
    $('.page-container-sample .btn-reset').click(function() {
      _([1,2,3,4]).forEach(function(i) {
        var spare = _.tail($(selector + '.thumbnail-' + i));
        if (spare.length) {
          $(spare).remove();
        }
      })
    });
  });
</script>
